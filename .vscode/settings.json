{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": false,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "node_modules/**": true,
    "package-lock.json": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    // vuepress's repo has this false ... why?
    "files.trimTrailingWhitespace": true,

    "editor.formatOnSave": false,
    "editor.renderWhitespace": "boundary",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "yaml.schemas": {
    // Warning: this doesn't actually work for anything but the main journyx_openapi.yaml
    // file; for all fragments it thinks you're trying to write a complete OpenAPI doc
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/schemas/v3.1/schema.json": "content/openapi/**/*.yaml"
  },
  "yaml.schemaStore.enable": true,
  "markdown.extension.toc.levels": "2..6",
  "markdown.extension.toc.unorderedList.marker": "-",
  "markdown.extension.toc.slugifyMode": "gitlab",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.orderedList.autoRenumber": false,
